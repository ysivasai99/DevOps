pipeline{
  agent any
  stages{
    stage("Git Clone"){
      steps{
        git branch: 'main', credentialsId: '70bc1ec1-8c14-41a9-ae7c-d8d219cb48b3', url: 'https://github.com/ysivasai99/DevOps.git'
      }
    }
    stage("Maven build"){
      steps{
        sh "mvn -f /var/lib/jenkins/workspace/Maven/Maven/webpage_feture/pom.xml clean package"
}
}
    }
}
